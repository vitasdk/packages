pkgname=xmp-lite
pkgver=9999
pkgrel=1
url="https://github.com/bucanero/libxmp-lite-ps4"
source=("git+https://github.com/bucanero/libxmp-lite-ps4.git")
sha256sums=('SKIP')

build() {
  cd libxmp-lite-ps4
  make -j$(nproc) -f Makefile.vita
}

package () {
  cd libxmp-lite-ps4
  make -f Makefile.vita install
}
