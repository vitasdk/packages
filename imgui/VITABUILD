pkgname=imgui
pkgver=9999
pkgrel=1
url="https://github.com/Rinnegatamante/imgui-vita"
source=("git://github.com/Rinnegatamante/imgui-vita.git")
sha256sums=('SKIP')
depends=('libmathneon' 'vitaGL')

build() {
  cd $pkgname-vita
  make -j$(nproc)
}

package () {
  cd $pkgname-vita
  make DESTDIR=$pkgdir install
}
