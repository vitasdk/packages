pkgname=libdl
pkgver=9999
pkgrel=1
url="https://github.com/hyln9/vita-libdl"
source=("git+https://github.com/hyln9/vita-libdl.git#commit=bf46551882c344e5aebee07f15267e2ca93d78a5")
sha256sums=('SKIP')
depends=('taihen')

build() {
  cd vita-libdl
  make PREFIX=$prefix
}

package () {
  cd vita-libdl
  make PREFIX=$prefix DESTDIR=$pkgdir install
}

