pkgname=bzip2
pkgver=9999
pkgrel=1
url="https://github.com/devnoname120/bzip2-vita"
source=("git://github.com/devnoname120/bzip2-vita.git")
sha256sums=('SKIP')

build() {
  cd bzip2-vita
  make libbz2.a
}

package () {
  cd bzip2-vita
  mkdir -p $pkgdir/$prefix/include
  mkdir -p $pkgdir/$prefix/lib
  make PREFIX=$pkgdir/$prefix installlib
}
