pkgname=un7zip
pkgver=9999
pkgrel=1
gitrev=bc2929c0c807ce5441dcba12cb02a5689113e995
url="https://github.com/bucanero/libun7zip"
source=("https://github.com/bucanero/libun7zip/archive/${gitrev}.tar.gz")
sha256sums=('SKIP')

build() {
  cd libun7zip
  make -j$(nproc) -f Makefile.vita
}

package () {
  cd libun7zip
  make -f Makefile.vita install
}
