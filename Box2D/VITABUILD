pkgname=Box2D
pkgver=9999
pkgrel=1
url="https://github.com/Rinnegatamante/Box2D/"
source=("git://github.com/Rinnegatamante/Box2D.git")
sha256sums=('SKIP')

build() {
  cd $pkgname
  make
}

package () {
  cd $pkgname
  mkdir -p $pkgdir/$prefix
  mkdir -p $pkgdir/$prefix/lib
  cp -r Include $pkgdir/$prefix
  cp libbox2d.a $pkgdir/$prefix/lib
}
